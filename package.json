{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public --no-clear",
    "start:dev": "sirv public --dev",
    "eslint:check": "eslint . --ext .svelte --ext .ts",
    "eslint:fix": "npm run eslint:check -- --fix",
    "prettier:check": "prettier --check ./**/*.{js,css,scss,html,json,*rc,svelte}",
    "prettier:write": "prettier --write ./**/*.{js,css,scss,html,json,*rc,svelte}",
    "precommit": "lint-staged & npm run build",
    "prepare": "husky install",
    "ts-dev": "tsc --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-typescript": "8.3.0",
    "@storybook/addon-actions": "6.4.4",
    "@storybook/addon-essentials": "6.4.4",
    "@storybook/addon-links": "6.4.5",
    "@storybook/addon-svelte-csf": "1.1.0",
    "@storybook/svelte": "6.4.4",
    "@tsconfig/svelte": "2.0.1",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "babel-loader": "8.2.3",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-svelte3": "3.2.1",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.0",
    "prettier-plugin-svelte": "2.5.0",
    "rollup": "2.60.2",
    "rollup-plugin-css-only": "3.1.0",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-svelte": "7.1.0",
    "rollup-plugin-terser": "7.0.2",
    "svelte": "3.44.2",
    "svelte-loader": "3.1.2",
    "svelte-preprocess": "4.9.8",
    "svelte-windicss-preprocess": "4.1.7",
    "typescript": "4.5.2",
    "windicss-webpack-plugin": "1.5.8"
  },
  "dependencies": {
    "sirv-cli": "1.0.14"
  },
  "engines": {
    "node": ">=12.13.0"
  }
}
