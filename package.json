{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public --no-clear",
    "start:dev": "sirv public --dev",
    "eslint:check": "eslint . --ext .svelte --ext .ts",
    "eslint:fix": "npm run eslint:check -- --fix",
    "prettier:check": "prettier --check ./**/*.{js,css,scss,html,json,*rc,svelte}",
    "prettier:write": "prettier --write ./**/*.{js,css,scss,html,json,*rc,svelte}",
    "precommit": "lint-staged & npm run build",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "sirv-cli": "2.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@rollup/plugin-commonjs": "22.0.1",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-typescript": "8.3.3",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/addon-svelte-csf": "2.0.4",
    "@storybook/svelte": "6.5.9",
    "@tsconfig/svelte": "3.0.0",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "babel-loader": "8.2.5",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.7.0",
    "rollup": "2.75.7",
    "rollup-plugin-css-only": "3.1.0",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-svelte": "7.1.0",
    "rollup-plugin-terser": "7.0.2",
    "svelte": "3.48.0",
    "svelte-loader": "3.1.3",
    "svelte-preprocess": "4.10.7",
    "svelte-windicss-preprocess": "4.2.8",
    "typescript": "4.7.4",
    "windicss-webpack-plugin": "1.7.3"
  },
  "engines": {
    "node": "^14.17.0 || >=16.0.0"
  }
}
